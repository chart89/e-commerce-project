// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Car {
  id String @id @default(uuid())
  mark String
  model String
  engine String
  HP Int
  price Int
  description String
}
